            ja = {
                {"索敵だって難しいんだ。いろんな状況判断が必要だからな。"}
            },
            en = {
                {"Ngay đến việc dò địch vốn cũng khó mà. Ta phải đánh giá vô thiên lủng các loại tình huống."}
            },
            cn = {
