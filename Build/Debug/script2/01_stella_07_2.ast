        text = {
            vo = {
                {"vo", ch = "c001", file = "z002400168"}
            },
            ja = {
                {"対して情報集めは……適当に電波なり履歴なりを拾えば、成立はする。問題は拾ったデータをどう分析するかだ」"}
            },
            en = {
                {"Mặt khác thì việc thu thập thông tin... nếu bắt được sóng điện và nhật trình phù hợp thì sẽ xoay xở được thôi. Vấn đề là làm sao để phân tích được lượng dữ liệu thu được ấy.\""}
            },
            cn = {
                {"而信息收集……只是随便截取些无线电或者历史记录也能算完成。问题在于如何去分析这些截取到的数据」"}
            }
        },
