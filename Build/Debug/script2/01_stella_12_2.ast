            ja = {
                {"「運び屋の仕事をやめて、身の丈に合った境遇に戻れば、追放を取り消すと迫られた。あいつもそうしろと勧めてきた。"}
            },
            en = {
                {"\"Tôi bị ra điều kiện phải bỏ nghề vận chuyển, rồi trở về với lối sống biết thân biết phận, làm thế thì lệnh trục xuất sẽ được gỡ. Cả cô ấy cũng khuyên tôi nghe theo."}
            },
            cn = {
                {"「他们力劝我放弃干货运人的工作，回去过我配过的生活，如此便可以撤销对我的驱逐。那个女人也劝我这么做。"}
            }
