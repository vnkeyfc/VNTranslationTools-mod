            ja = {
                {"「娘だった。まだ俺たち自身が子供のようなもんだったが、稼ぎがあったから、どうにかなると思ってた」"}
            },
            en = {
                {"\"Là con gái. Khi ấy chúng tôi hẵng còn trẻ người non dạ, nhưng cũng có thu nhập rồi nên cứ nghĩ sẽ cáng đáng ổn thỏa thôi.\""}
            },
            cn = {
