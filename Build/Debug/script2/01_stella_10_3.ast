    block_00157 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c001", file = "z003800199"}
            },
            ja = {
                {"「公爵との意見違いがあったんだろう。親父さんの夢と公爵の夢が、食い違った。"}
            },
            en = {
                {"\"Có lẽ giữa cha nhóc và lão Công tước đã có bất đồng ý kiến. Ước mơ của anh ta và Công tước phát sinh mâu thuẫn."}
            },
            cn = {
                {"「我想是你们和公爵之间观点有分歧。你父亲的梦想和公爵的梦想产生了冲突。"}
            }
        },
        linkback = "block_00156",
        linknext = "block_00158",
        line = 387
    },
    block_00158 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c001", file = "z003800200"}
            },
            ja = {
                {"夢を追うような人間だ。理想も高かったんじゃないか？"}
