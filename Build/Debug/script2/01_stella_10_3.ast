        text = {
            vo = {
                {"vo", ch = "c001", file = "z003800180"}
            },
            ja = {
                {"「でも君の親父さんは違うみたいだ。夢があったんだな。"}
            },
            en = {
                {"\"Nhưng cha nhóc có vẻ khác. Anh ta có ước mơ."}
            },
            cn = {
                {"「但你的父亲似乎不同，他拥有着梦想。"}
            }
        },
        linkback = "block_00139",
        linknext = "block_00141",
        line = 368
    },
    block_00141 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c001", file = "z003800181"}
            },
            ja = {
                {"語るだけで動かない夢想家を、俺は否定するよ。けど夢に向かって歩いていく夢想家には、何も言えなくなる」"}
            },
            en = {
                {"Tôi bác bỏ tất thảy những kẻ chỉ biết mơ tưởng mà không mó tay làm gì. Chứ còn những người luôn nỗ lực hướng tới lý tưởng thì tôi không có gì để nói.\""}
            },
            cn = {
                {"没有行动只会空谈的梦想家，我会否定。但是朝着梦想前进的梦想家，能让我无可反驳」"}
            }
        },
        linkback = "block_00140",
        linknext = "block_00142",
        line = 369
    },
    block_00142 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c004", file = "z003800182"}
            },
            ja = {
                {"「……ジュードさんにひとつ、お話しておきます」"}
            },
            en = {
                {"\"...... Cháu có một chuyện này, muốn nói với Jude-san.\""}
            },
            cn = {
                {"「……我想和裘德说一件事情」"}
            }
        },
        linkback = "block_00141",
        linknext = "block_00143",
        line = 370
    },
    block_00143 = {
        {"text"},
        text = {
            pagebreak = true,
            vo = {
                {"vo", ch = "c001", file = "z003800183"}
            },
            ja = {
                {"「うかがおう」"}
            },
            en = {
                {"\"Nói đi.\""}
            },
            cn = {
                {"「请说吧」"}
            }
        },
        linkback = "block_00142",
        linknext = "block_00144",
        line = 371
    },
    block_00144 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c004", file = "z003800184"}
            },
            ja = {
                {"「実は私と父ですが、一度はグロウナー公爵のもとに行ったんです」"}
            },
            en = {
                {"\"Thực ra cháu và cha, đã từng đến chỗ Công tước Grosvenor một lần rồi.\""}
            },
            cn = {
                {"「其实，我和父亲曾经去过格罗夫纳公爵那里」"}
            }
        },
        linkback = "block_00143",
        linknext = "block_00145",
        line = 372
    },
    block_00145 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c001", file = "z003800185"}
            },
            ja = {
                {"「……なんだって？」"}
            },
            en = {
                {"\"...... Sao chứ?\""}
            },
            cn = {
                {"「……你说什么？」"}
            }
        },
        linkback = "block_00144",
        linknext = "block_00146",
        line = 373
    },
    block_00146 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c004", file = "z003800186"}
            },
            ja = {
                {"「ですが、引き返して半島に戻ってきました。"}
            },
            en = {
                {"\"Nhưng rồi, chúng cháu lại trở về bán đảo này."}
            },
            cn = {
                {"「但是，我们又折返回到了半岛。"}
            }
        },
        linkback = "block_00145",
        linknext = "block_00147",
        line = 374
    },
    block_00147 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c004", file = "z003800187"}
            },
            ja = {
                {"男たちに囚われたのはその時なんです。おふたりのことを信頼しきれず、つい話を伏せてしまいました」"}
            },
            en = {
                {"Đó cũng là lúc những người kia bắt được cháu. Ban đầu cháu chưa thật sự tin tưởng hai người, nên thành ra phải giấu nhẹm đi.\""}
            },
            cn = {
                {"就是那时我被那些男人抓住了。因为当时没有完全信任你们，所以没有说出实情」"}
            }
        },
        linkback = "block_00146",
        linknext = "block_00148",
        line = 375
    },
    block_00148 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c002", file = "z003800188"}
            },
            ja = {
                {"「公爵さんのところに行ったの？」"}
            },
            en = {
                {"\"Chị từng tới chỗ ông Công tước rồi sao?\""}
            },
            cn = {
                {"「你去了公爵先生那里吗？」"}
            }
        },
        linkback = "block_00147",
        linknext = "block_00149",
        line = 376
    },
    block_00149 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c004", file = "z003800189"}
            },
            ja = {
                {"「行ったのですが、その記憶が私にはありません"}
            },
            en = {
                {"\"Chị đã tới rồi, nhưng không lưu lại ký ức nào cả."}
            },
            cn = {
                {"「去过，但我已经没有相关的记忆了」"}
            }
        },
        linkback = "block_00148",
        linknext = "block_00150",
        line = 377
    },
    block_00150 = {
        {"text"},
        text = {
            pagebreak = true,
            vo = {
                {"vo", ch = "c004", file = "z003800190"}
            },
            ja = {
                {"銃撃戦で頭を撃たれた際、記憶ごと削れてしまったようです」"}
            },
            en = {
                {"Lúc đấu súng, chị bị một viên đạn găm vào đầu, thành ra ký ức bị xóa đi cả.\""}
            },
            cn = {
                {"枪战中我的头部被击中的时候，记忆也一同消失不见了」"}
            }
        },
        linkback = "block_00149",
        linknext = "block_00151",
        line = 378
    },
    block_00151 = {
        {"text"},
        text = {
            pagebreak = true,
            ja = {
                {"デリラは自分の破損した頭部を、震える右腕で示した。"}
            },
            en = {
                {"Delilah dùng cánh tay phải run rẩy trỏ vào phần đầu bị thương của mình."}
            },
            cn = {
                {"黛莉拉用颤抖的右臂指着自己破损的头部。"}
            }
        },
        linkback = "block_00150",
        linknext = "block_00152",
        line = 380
    },
    block_00152 = {
        {"text"},
        text = {
            vo = {
                {"vo", ch = "c004", file = "z003800194"}
            },
            ja = {
                {"「公爵に会ったのか、会っていないのか……そしてなぜ引き返すことになったのか、いきさつは覚えていません……」"}
            },
            en = {
                {"\"Liệu chị đã gặp Công tước hay chưa...... hay tại sao chị lại trở về, chị hoàn toàn chẳng nhớ gì cả......\""}
            },
            cn = {
                {"「我有没有见到公爵……还有为什么又折返回来，前前后后什么都不记得了……」"}
            }
        },
        linkback = "block_00151",
        linknext = "block_00153",
        line = 382
    },
